{
   "name": "wasaby-cli",
   "version": "21.2000.0",
   "description": "Build and run unit tests for wasaby",
   "scripts": {
      "test": "mocha ./test --recursive",
      "test:coverage": "nyc --reporter=html npm run test",
      "install-selenium": "npm install selenium-standalone@6.14.x && node node_modules/selenium-standalone/bin/selenium-standalone install",
      "tslint": "tslint -q app/**/*.js"
   },
   "repository": {
      "type": "git",
      "url": "git@github.com:saby/wasaby-cli.git"
   },
   "bin": {
      "wasaby-cli": "./cli.js"
   },
   "dependencies": {
      "body-parser": "^1.18.3",
      "cookie-parser": "^1.4.3",
      "express": "^4.16.3",
      "fs-extra": "^7.0.1",
      "p-map": "^2.1.0",
      "saby-typescript": "git+https://github.com/saby/TypeScript.git#rc-21.2000",
      "saby-units": "git+https://github.com/saby/Units.git#rc-21.2000",
      "sbis3-builder": "git+https://github.com/saby/Builder.git#rc-21.2000",
      "serve-static": "^1.14.1",
      "service-worker-mock": "^2.0.5",
      "webdriverio": "5.16.6",
      "xml2js": "^0.4.19"
   },
   "devDependencies": {
      "chai": "^4.2.0",
      "mocha": "^6.1.4",
      "nyc": "^12.0.2",
      "sinon": "^7.5.0"
   }
}