{
   "store": "store",
   "workDir": "application",
   "repositories": {
      "engine": {
         "url": "https://git.sbis.ru/sbis/engine.git",
         "unitInBrowser": true,
         "test": "test/unit",
         "modules": [
            "client/LongOperations",
            "client/SchemeEditor",
            "client/ParametersWebAPI",
            "client/Graphs",
            "client/RichEditor",
            "client/Previewer",
            "client/Summation",
            "client/Layout",
            "client/NavigationPanels",
            "client/ExtControls",
            "client/EngineLink",
            "client/EngineUser",
            "client/EngineLanguage"
         ]
      },
      "types": {
         "url": "https://platform-git.sbis.ru/saby/Types.git",
         "test": "test",
         "modules": ["Types"],
         "dependTest": ["controls", "ws", "router", "ui", "app", "rmi", "engine"]
      },
      "i18n": {
         "url": "https://platform-git.sbis.ru/saby/I18n.git",
         "test": "test",
         "unitInBrowser": true,
         "modules": ["I18n"],
         "dependTest": ["types", "controls", "router", "ui", "app", "rmi", "engine"]
      },
      "ws": {
         "url": "https://git.sbis.ru/sbis/ws.git",
         "test": "test/unit",
         "modules": ["WS.Core", "test/WSTests", "Vdom", "View", "Inferno", "WS.Deprecated", "test/unit", "Demo"],
         "unitInBrowser": true,
         "dependTest": ["types", "controls", "router", "ui", "app", "rmi", "engine"]
      },
      "controls": {
         "url": "https://git.sbis.ru/sbis/controls.git",
         "unitInBrowser": true,
         "test": "tests/unit",
         "dependTest": ["engine"],
         "modules": ["SBIS3.CONTROLS", "Controls", "tests/unit", "Examples", "Controls-demo"]
      },
      "rmi": {
         "url": "https://git.sbis.ru/sbis/rmi.git",
         "test": "tests/unit",
         "unitInBrowser": true,
         "modules": [
            "src/client/Browser",
            "src/client/Env",
            "src/client/File",
            "src/client/File-demo",
            "src/client/SbisEnv",
            "src/client/SbisFile",
            "tests"
         ],
         "dependTest": ["router", "types", "engine"]
      },
      "app": {
         "url": "https://platform-git.sbis.ru/saby/wasaby-app.git",
         "unitInBrowser": true,
         "modules": [
            "src/Application"
         ],
         "dependTest": ["engine"],
         "test": "test"
      },
      "router": {
         "url": "https://platform-git.sbis.ru/saby/router",
         "unitInBrowser": true,
         "modules": [
            "RouterDemo", "Router", "RouterTest"
         ],
         "dependTest": ["engine"],
         "test": "RouterTest"
      },
      "ui": {
         "url": "https://platform-git.sbis.ru/saby/ui",
         "unitInBrowser": true,
         "modules": [
            "UI", "UIDemo", "UITest"
         ],
         "dependTest": ["engine"],
         "test": "UITest"
      },
      "cdn": {
         "url": "https://platform-git.sbis.ru/saby/wasaby-cdn",
         "linkFolders": {
            "/" : "cdn"
         },
         "modules": []
      }
   }
}



