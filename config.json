{
   "store": "store",
   "workDir": "application",
   "repositories": {
      "engine": {
         "url": "https://git.sbis.ru/sbis/engine.git",
         "unitInBrowser": true,
         "module": ["EngineLink"],
         "test": "test/unit"
      },
      "types": {
         "url": "https://platform-git.sbis.ru/saby/Types.git",
         "test": "test"
      },
      "i18n": {
         "url": "https://platform-git.sbis.ru/saby/I18n.git",
         "test": "test",
         "unitInBrowser": true
      },
      "ws": {
         "url": "https://git.sbis.ru/sbis/ws.git",
         "test": "test/unit",
         "modules": ["test/WSTests", "test/unit", "Demo"],
         "unitInBrowser": true
      },
      "controls": {
         "url": "https://git.sbis.ru/sbis/controls.git",
         "unitInBrowser": true,
         "test": "tests/unit",
         "modules": ["tests/unit"]
      },
      "rmi": {
         "url": "https://git.sbis.ru/sbis/rmi.git",
         "test": "tests/unit",
         "unitInBrowser": true,
         "linkFolders": {
            "/tests/unit" : "tests"
         }
      },
      "app": {
         "url": "https://platform-git.sbis.ru/saby/wasaby-app.git",
         "unitInBrowser": true,
         "test": "test"
      },
      "router": {
         "url": "https://platform-git.sbis.ru/saby/router.git",
         "unitInBrowser": true,
         "dependOn":["ui"],
         "test": "RouterTest"
      },
      "ui": {
         "url": "https://platform-git.sbis.ru/saby/ui.git",
         "unitInBrowser": true,
         "dependOn":["controls"],
         "test": "UITest"
      },
      "cdn": {
         "url": "https://platform-git.sbis.ru/saby/wasaby-cdn.git",
         "linkFolders": {
            "/" : "cdn"
         },
         "modules": []
      },
      "schemeeditor": {
         "url": "https://git.sbis.ru/engine/schemeeditor.git",
         "unitInBrowser": true,
         "dependOn":["rmi", "controls", "ui"],
         "test": "tests/unit/SchemeEditor"
      },
      "viewsettings": {
         "url": "https://git.sbis.ru/engine/viewsettings.git"
      },
      "plugin-client": {
         "url": "https://git.sbis.ru/sbis/plugin-client.git"
      },
      "navigation-configuration": {
         "url": "https://git.sbis.ru/navigation-configuration/navigation.git"
      }
   }
}



